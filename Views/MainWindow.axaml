<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:m="using:Sharp.Studio.Models"
        xmlns:v="using:Sharp.Studio.Views"
        xmlns:vm="using:Sharp.Studio.ViewModels"
        xmlns:vms="using:Sharp.DockManager.ViewModels"
        xmlns:dc="using:Sharp.DockManager"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Sharp.Studio.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="Sharp.Studio">

    <Design.DataContext>
        <vm:MainWindowViewModel/>
    </Design.DataContext>

  <dc:DockableTabControl>
    <dc:DockableTabControl.OptionalHeaderAttachment>
      <Button Content="+"/>
    </dc:DockableTabControl.OptionalHeaderAttachment>
      <dc:DockableTabControl.ItemsSource>
        <m:DockableAssets/>
        <m:DockableAssets/>
        <m:DockableScene/>
        <m:DockableAssets/>
        <m:DockableAssets/>
        <m:DockableScene/>
      </dc:DockableTabControl.ItemsSource>
    </dc:DockableTabControl>


</Window>
