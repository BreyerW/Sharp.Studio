<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:s="using:System"
			 xmlns:local="using:Sharp.Studio"
             xmlns:v="using:Sharp.Studio.Views"
             xmlns:vm="using:Sharp.DockManager.Models"
             xmlns:vms="using:Sharp.DockManager.ViewModels"
             xmlns:dt="using:Sharp.DockManager"
             xmlns:behaviours="using:Sharp.DockManager.Behaviours"
			 x:Class="Sharp.Studio.App"
			 RequestedThemeVariant="Dark">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  <Application.Resources>
    <DataTemplate x:Key="AssetsDock">
      <vms:DockableItem>
        <vms:DockableItem.Header>
          <TextBlock Text="Assets"/>
        </vms:DockableItem.Header>
        <vms:DockableItem.Content>
          <v:AssetsView/>
        </vms:DockableItem.Content>
      </vms:DockableItem>
    </DataTemplate>
  </Application.Resources>
    <Application.Styles>
      
        <FluentTheme/>
    </Application.Styles>
</Application>
